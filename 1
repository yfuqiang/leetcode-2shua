class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int> m;
        vector<int> res(2,-1);
        int i;
        for(i=0;i<nums.size();i++)
        {
            if(m.count(target-nums[i])!=0)
            {
                res[0]=m[target-nums[i]];
                res[1]=i;
                break;
            }
            m[nums[i]]=i;
        }
        return res;
    }
};
